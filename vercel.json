{
  "rewrites": [
    {
      "source": "/",
      "destination": "/Webseite/index.html"
    },
    {
      "source": "/install.mobileconfig",
      "destination": "/Webseite/install.mobileconfig"
    },
    {
      "source": "/iOS/:path*",
      "destination": "/iOS/:path*"
    },
    {
      "source": "/App/:path*",
      "destination": "/App/:path*"
    }
  ],
  "headers": [
    {
      "source": "/Webseite/install.mobileconfig",
      "headers": [
        { "key": "Content-Type", "value": "application/x-apple-asig-config" }
      ]
    }
  ]
}