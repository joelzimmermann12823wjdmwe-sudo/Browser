{
  "name": "aetheros-browser",
  "productName": "AetherOS",
  "version": "1.0.0",
  "description": "Next-Gen Gaming Browser",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "make": "electron-forge make",
    "package": "electron-forge package"
  },
  "author": "Joel Zimmermann",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "aetheros-browser"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "aetheros_browser",
            "setupExe": "aetheros-setup.exe"
          }
        }
      ]
    }
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.7.3",
    "update-electron-app": "^3.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.0.0",
    "@electron-forge/maker-squirrel": "^7.0.0",
    "@electron/rebuild": "^4.0.3",
    "electron": "^40.1.0"
  }
}
